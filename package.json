{
  "name": "vite-typescript-npm-package",
  "version": "1.0.0",
  "description": "Vite TypeScript library npm package template",
  "license": "ISC",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite --host --open",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "watch": "tsc && vite build --watch",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "start-integration-test": "wait-on tcp:5173 && jest && npx kill-port 5173",
    "start-test-server": "vite --host",
    "test": "concurrently  \"npm:start-test-server\" \"npm:start-integration-test\""
  },
  "dependencies": {
    "daikon": "^1.2.42",
    "fflate": "^0.7.4",
    "gl-matrix": "^3.4.3",
    "gltf-loader-ts": "^0.3.1",
    "nifti-reader-js": "^0.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@jest/globals": "^29.4.3",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-jest": "^29.4.3",
    "concurrently": "^7.6.0",
    "dts-bundle-generator": "^7.1.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.1",
    "puppeteer": "^19.7.3",
    "puppeteer-utilz": "^4.0.8",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vite-plugin-glsl": "^1.1.2",
    "wait-on": "^7.0.1"
  }
}
